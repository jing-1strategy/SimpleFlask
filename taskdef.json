{
    "executionRoleArn": "arn:aws:iam::842337631775:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "simpleflask-bluegreen-container",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "simpleflask-bluegreen-taskdef"
}